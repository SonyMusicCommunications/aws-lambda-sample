{
  "name": "hello-lambda",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "_pre": "npm run sass -- --output-style compressed && npm run pug",
    "_aot": "npm run sass -- --output-style expanded && npm run pug -- -P",
    "pug": "pug src --doctype html",
    "sass": "node-sass src -o src",
    "index": "npm run _aot && npm run ng -- build --aot",
    "lambda": "npm run ng -- build lambda",
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run _pre && npm run ng -- build --prod && npm run ng -- build lambda --prod",
    "test": "ng test",
    "lint": "ng lint --format stylish",
    "e2e": "ng e2e"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.6.8",
    "@angular/animations": "^6.0.6",
    "@angular/cli": "^6.0.8",
    "@angular/common": "^6.0.6",
    "@angular/compiler": "^6.0.6",
    "@angular/compiler-cli": "^6.0.6",
    "@angular/core": "^6.0.6",
    "@angular/forms": "^6.0.6",
    "@angular/http": "^6.0.6",
    "@angular/language-service": "^6.0.6",
    "@angular/platform-browser": "^6.0.6",
    "@angular/platform-browser-dynamic": "^6.0.6",
    "@angular/router": "^6.0.6",
    "@types/jasmine": "^2.8.8",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^10.3.6",
    "aws-amplify": "^0.4.6",
    "aws-sdk": "^2.263.1",
    "codelyzer": "^4.4.2",
    "core-js": "^2.5.7",
    "jasmine-core": "^3.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^2.0.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.1",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.1.0",
    "protractor": "^5.3.2",
    "pug-cli": "^1.0.0-alpha6",
    "rxjs": "^6.2.1",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "typescript": "2.7.2",
    "zone.js": "^0.8.26"
  }
}
